    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.17",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  }
